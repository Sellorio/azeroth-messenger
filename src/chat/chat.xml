					<Color r="0.65" g="0.44" b="0.65" a="1"/>
                </Texture>
			</Layer>
            <Layer level="OVERLAY">
                <FontString parentKey="TitleLabel" inherits="Number15Font">
                    <Size x="220" y="15"/>
                    <Anchors>
						<Anchor point="TOP" x="0" y="-13"/>
